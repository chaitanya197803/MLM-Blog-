{
    "version": 2,
    "public": true,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.2",
            "includeFiles": "mlm-system-core/**"
        }
    },
    "routes": [
        {
            "src": "/(build|css|js|images|favicon.ico|manifest.json)(.*)",
            "dest": "/mlm-system-core/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr"
    }
}